//new task
//exponent (** double multipication)

let bags: number = 5;
let apples: number = 5;
let totalapples: number = bags ** apples;

console.log(totalapples);

//modulus % percentage

let apple: number = 5
let bags: number = 2
let remainder = apple % bags

console.log(remainder)

//combbines operators (use  more then 2 operators use at a time)

